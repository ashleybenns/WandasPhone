<?xml version="1.0" encoding="utf-8"?>
<!--
    Pre-Android 12 (API < 31) backup rules.
    
    SECURITY: This app stores sensitive user data that must NOT be backed up.
    See backup_rules.xml for full documentation.
    
    This file is used on Android 11 and below.
-->
<full-backup-content>
    <!-- Exclude everything - no backup allowed -->
    <exclude domain="sharedpref" path="." />
    <exclude domain="database" path="." />
    <exclude domain="file" path="." />
    <exclude domain="external" path="." />
    <exclude domain="root" path="." />
</full-backup-content>
